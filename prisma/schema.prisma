// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model License {
  id                Int      @id @default(autoincrement())
  Contratto         String?
  Produttore        String
  Prodotto          String
  Tipo_Licenza      String
  Numero_Licenze    Int
  Bundle            Int
  Borrowable        Boolean
  Rivenditore       String?
  Email_Rivenditore String?  @db.VarChar(255)
  Scadenza          DateTime?
}
