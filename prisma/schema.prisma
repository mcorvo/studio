
datasource db {
  provider = "postgresql" // Or your chosen provider like "mysql", "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["rhel-openssl-3.0.x", "linux-musl-openssl-3.0.x"]
}

model License {
  id             Int     @default(autoincrement()) @id
  Produttore     String
  Prodotto       String
  Tipo_Licenza   String
  Numero_Licenze Int
  Bundle         Int
  Borrowable     Boolean
  Rivenditore    String
  Contratto      String
  Scadenza       DateTime
}
